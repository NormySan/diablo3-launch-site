# Server specifics, so we don't include them
.htaccess
/sites/*/settings.local.php

# User generated content should not be in the repository
/sites/*/files/*
/sites/*/tmp/*

# Include README in the files folders.
!/sites/*/files/README.txt

# Exlude various other files
icon.png
icon.gif
icon.jpg
.DS_Store
.sass-cache

# Do not include node modules.
/sites/all/themes/*/node_modules
/sites/all/themes/*/bower_components
